# Supabase Self-Hosted Configuration
#
# Copy this file to .env and customize the values.
# These environment variables are used by docker-compose.supabase.yml

############
# Secrets
# YOU MUST CHANGE THESE VALUES IN PRODUCTION
############

# PostgreSQL password (must be at least 20 characters)
POSTGRES_PASSWORD=your-super-secret-password

# JWT Secret (must be at least 32 characters)
# You can generate one with: openssl rand -base64 32
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters

# JWT Expiration time in seconds (default: 1 hour)
JWT_EXP=3600

############
# Keys
# You can generate new keys at: https://supabase.com/docs/guides/self-hosting#generate-api-keys
# Or use these demo keys for local development only
############

# Anonymous key - used for public operations (login, signup, etc.)
# This is a demo key - DO NOT USE IN PRODUCTION
ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24iLCJleHAiOjE5ODM4MTI5OTZ9.CRXP1A7WOeoJeXxjNni43kdQwgnWNReilDMblYTn_I0

# Service role key - has admin privileges, keep secret!
# This is a demo key - DO NOT USE IN PRODUCTION
SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6InNlcnZpY2Vfcm9sZSIsImV4cCI6MTk4MzgxMjk5Nn0.EGIM96RAZx35lJzdJsyH-qQwv8Hdp7fsn3W0YpN81IU

############
# Dashboard Credentials
############

DASHBOARD_USERNAME=supabase
DASHBOARD_PASSWORD=this_password_is_insecure_and_should_be_updated

############
# URLs
############

# Public URL where Supabase is accessible
API_EXTERNAL_URL=http://localhost:8000
SUPABASE_PUBLIC_URL=http://localhost:8000

# Your application's URL (for redirects after auth)
SITE_URL=http://localhost:3000

# Additional redirect URLs (comma-separated)
ADDITIONAL_REDIRECT_URLS=

############
# Auth Configuration
############

# Disable signups (set to true to only allow invited users)
DISABLE_SIGNUP=false

# Enable email signup
ENABLE_EMAIL_SIGNUP=true

# Auto-confirm email addresses (false = send verification emails)
# Local dev with Inbucket: set to false to test email flow
# Production: should be false to require email verification
ENABLE_EMAIL_AUTOCONFIRM=false

# Enable anonymous users
ENABLE_ANONYMOUS_USERS=false

############
# SMTP Configuration (Email for Authentication)
############
# LOCAL DEV: Emails are captured by Inbucket at http://localhost:54324
#            No configuration needed - works out of the box!
#
# PRODUCTION: Get a free Resend account at https://resend.com
#             Free tier: 3,000 emails/month, 100 emails/day
#
# Resend SMTP Settings:
# SMTP_HOST=smtp.resend.com
# SMTP_PORT=465
# SMTP_USER=resend
# SMTP_PASS=re_your_api_key_here
# SMTP_ADMIN_EMAIL=noreply@yourdomain.com

############
# Studio Configuration
############

STUDIO_DEFAULT_ORGANIZATION=QCKSTRT
STUDIO_DEFAULT_PROJECT=qckstrt

############
# Database Schemas
############

# Schemas exposed via PostgREST
PGRST_DB_SCHEMAS=public,storage,graphql_public
