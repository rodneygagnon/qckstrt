{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "pnpm lint",
    "format": "prettier --write \"app/**/*.tsx\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
