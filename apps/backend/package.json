{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm build:organizations && pnpm build:persons && pnpm build:roles",
    "build:organizations": "nest build --tsc organizations",
    "build:persons": "nest build --tsc persons",
    "build:roles": "nest build --tsc roles",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "concurrently \"pnpm start:orgs\" \"pnpm start:persons\" \"pnpm start:roles\"",
    "start:organizations": "pnpm build:organizations && nest start organizations --watch",
    "start:organizations:debug": "pnpm build:organizations && nest start organizations --debug --watch",
    "start:persons": "pnpm build:persons && nest start persons --watch",
    "start:persons:debug": "pnpm build:persons && nest start persons --debug --watch",
    "start:roles": "pnpm build:roles && nest start roles --watch",
    "start:roles:debug": "pnpm build:roles && nest start roles --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/jest-e2e.json"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.15.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "aws-lambda": "^1.0.7",
    "helmet": "^7.1.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
