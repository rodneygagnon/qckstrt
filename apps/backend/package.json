  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm build:api && pnpm build:users && pnpm build:documents && pnpm build:knowledge",
    "build:documents": "nest build --tsc documents",
    "build:knowledge": "nest build --tsc knowledge",
    "build:users": "nest build --tsc users",
    "build:api": "nest build --tsc api",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "concurrently \"pnpm start:api\" \"pnpm start:users\" \"pnpm start:documents\" \"pnpm start:knowledge\"",
    "start:documents": "pnpm build:documents && NODE_ENV=dev nest start documents --watch",
    "start:documents:debug": "pnpm build:documents && NODE_ENV=dev nest start documents --debug --watch",
    "start:knowledge": "pnpm build:knowledge && NODE_ENV=dev nest start knowledge --watch",
    "start:knowledge:debug": "pnpm build:knowledge && NODE_ENV=dev nest start knowledge --debug --watch",
    "start:users": "pnpm build:users && NODE_ENV=dev nest start users --watch",
    "start:users:debug": "pnpm build:users && NODE_ENV=dev nest start users --debug --watch",
    "start:api": "pnpm build:api && NODE_ENV=dev nest start api --watch",
    "start:api:debug": "pnpm build:api && NODE_ENV=dev nest start api --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/jest-e2e.json",
    "preinstall": "npx only-allow pnpm"
  },
