{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/organizations/src",
  "entryFile": "main",
  "monorepo": true,
  "root": "apps/organizations",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/organizations/tsconfig.app.json",
    "deleteOutDir": true
  },
  "projects": {
    "organizations": {
      "type": "application",
      "root": "src/apps/organizations",
      "entryFile": "main",
      "sourceRoot": "src/apps/organizations/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/organizations/tsconfig.app.json"
      }
    },
    "persons": {
      "type": "application",
      "root": "src/apps/persons",
      "entryFile": "main",
      "sourceRoot": "src/apps/persons/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/persons/tsconfig.app.json"
      }
    },
    "roles": {
      "type": "application",
      "root": "src/apps/roles",
      "entryFile": "main",
      "sourceRoot": "src/apps/roles/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/roles/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "src/apps/users",
      "entryFile": "main",
      "sourceRoot": "src/apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/users/tsconfig.app.json"
      }
    },
    "posts": {
      "type": "application",
      "root": "src/apps/posts",
      "entryFile": "main",
      "sourceRoot": "src/apps/posts/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/posts/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "src/api",
      "entryFile": "main",
      "sourceRoot": "src/api/src",
      "compilerOptions": {
        "tsConfigPath": "src/api/tsconfig.app.json"
      }
    }
  }
}
