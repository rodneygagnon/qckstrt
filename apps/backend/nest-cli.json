{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/organizations/src",
  "entryFile": "main",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json",
    "deleteOutDir": true
  },
  "projects": {
    "users": {
      "type": "application",
      "root": "src/apps/users",
      "entryFile": "main",
      "sourceRoot": "src/apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/users/tsconfig.app.json"
      }
    },
    "documents": {
      "type": "application",
      "root": "src/apps/documents",
      "entryFile": "main",
      "sourceRoot": "src/apps/documents/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/documents/tsconfig.app.json"
      }
    },
    "knowledge": {
      "type": "application",
      "root": "src/apps/knowledge",
      "entryFile": "main",
      "sourceRoot": "src/apps/knowledge/src",
      "compilerOptions": {
        "tsConfigPath": "src/apps/knowledge/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "src/api",
      "entryFile": "main",
      "sourceRoot": "src/api/src",
      "compilerOptions": {
        "tsConfigPath": "src/api/tsconfig.app.json"
      }
    }
  }
}
